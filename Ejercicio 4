//Cambia las variables 1->2, 2->3 y 3->1
#include <stdio.h>

int main(){
int v1, v2, v3;    
    
printf("Ingrese el 1º valor: ");
scanf("%d",&v1);
printf("Ingrese el 2º valor: ");
scanf("%d",&v2);
printf("Ingrese el 3º valor: ");
scanf("%d",&v3);

v1 = v1+v2;
v2 = v1-v2;
v1 = v1-v2;

v2 = v2+v3;
v3 = v2-v3;
v2 = v2-v3;

printf("\nEl 1º valor %d", v1);
printf("\nEl 2º valor %d", v2);
printf("\nEl 3º valor %d", v3);

return 0;	
}
